# This is Git's per-user configuration file.
[user]
	email = graham@ghall.dev
	name = Graham Hall
[core]
	editor = micro
	excludesfile = /Users/ghall/.gitignore
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[init]
	defaultBranch = main
